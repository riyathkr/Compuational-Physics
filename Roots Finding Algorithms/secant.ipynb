    "def func(x):\n",
    "    return(x**2-1)\n",
    "def secant(x1,x2,iterations):\n",
    "    for i in range(iterations):\n",
    "        x3=x2-func(x2)*(x2-x1)/(func(x2)-func(x1))\n",
    "        x1,x2=x2,x3\n",
    "    return x3"
   ]
